{
  "name": "node-scaffolding-tool",
  "version": "1.0.0",
  "description": "",
  "bin": {
    "init-node-app": "./dist/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "start": "node --inspect=0.0.0.0:5858 -r ts-node/register ./src/index.ts",
    "dev": "nodemon",
    "build:watch": "nodemon --exec yarn build",
    "build": "tsc",
    "test": "jest",
    "docker": "docker-compose up --build",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker/dev.yml up --build"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbiskur/node-scaffolding-tool.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jbiskur/node-scaffolding-tool/issues"
  },
  "homepage": "https://github.com/jbiskur/node-scaffolding-tool#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/core-js": "^2.5.0",
    "@types/inquirer": "^6.0.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.6",
    "@types/ora": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "core-js": "^3.0.1",
    "inquirer": "^6.3.1",
    "lodash": "^4.17.11",
    "ora": "^3.4.0"
  }
}
